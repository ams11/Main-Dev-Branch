= form_for @storeconfig do |f|
  -if @storeconfig.errors.any?
    #error_explanation
      %h2= "#{pluralize(@storeconfig.errors.count, "error")} prohibited this storeconfig from being saved:"
      %ul
        - @storeconfig.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.text_field :user_id
  .field
    = f.label :display_name
    = f.text_field :display_name
  .actions
    = f.submit 'Save'
